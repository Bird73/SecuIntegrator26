{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=secuintegrator.db"
  },
  "Quartz": {
    "quartz.scheduler.instanceName": "SecuIntegratorScheduler",
    "quartz.jobStore.type": "Quartz.Simpl.RAMJobStore, Quartz",
    "quartz.threadPool.maxConcurrency": "5"
  },
  "Playwright": {
    "WorkerPath": "src/SecuIntegrator26.PlaywrightWorker/bin/Debug/net8.0/SecuIntegrator26.PlaywrightWorker.exe"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.json",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "retainedFileCountLimit": 31
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "LineNotify": {
    "Token": "YOUR_LINE_NOTIFY_TOKEN_HERE"
  },
  "AllowedHosts": "*"
}